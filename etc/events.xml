<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
	<event name="payment_method_assign_data_fiserv_commercehub">
		<observer name="fiserv_commercehub_data_assign" instance="Fiserv\Payments\Observer\CommerceHub\DataAssignObserver" />
	</event>
	<event name="adminhtml_sales_order_create_process_data">
        <observer name="fiserv_valuelink" instance="Fiserv\Payments\Observer\Valuelink\ProcessOrderCreationData" />
    </event>
	<event name="sales_model_service_quote_submit_before">
        <observer name="fiserv_valuelink" instance="Fiserv\Payments\Observer\Valuelink\ProcessOrderPlace" />
	</event>
	<event name="checkout_submit_all_after">
		<observer name="fiserv_valuelink_sale_invoice" instance="Fiserv\Payments\Observer\Valuelink\ValuelinkOnlySaleCreateInvoice" />
		<observer name="fiserv_valuelink_order_enrich" instance="Fiserv\Payments\Observer\Valuelink\EnrichValuelinkTransactionsWithOrderId" />
	</event>
	<event name="sales_order_payment_cancel">
        <observer name="fiserv_valuelink" instance="Fiserv\Payments\Observer\Valuelink\CancelValuelinkTransactions" />
	</event>
	<event name="sales_order_payment_void">
        <observer name="fiserv_valuelink" instance="Fiserv\Payments\Observer\Valuelink\CancelValuelinkTransactions" />
	</event>
 	<event name="sales_order_invoice_save_after">
        <observer name="fiserv_valuelink" instance="Fiserv\Payments\Observer\Valuelink\EnrichValuelinkTransactionsWithInvoiceId" />
    </event>
</config>
